        <!-- right content column-->
        <div class="col-md-12" id="content">
            <div class="panel">
                <div class="panel-heading top-title">{% block page_title %}{% endblock %}</div>
                <div class="panel-body">
                    {% block body %} {% endblock %}
                </div><!--/panel-body-->
            </div><!--/panel-->
            <!--/end right column-->
        </div>
    </div>
</div>
